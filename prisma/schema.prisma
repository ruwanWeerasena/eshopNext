// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Customer {
  id            Int      @id @default(autoincrement())
  
  email         String
  username      String
  image         String
  firstname     String
  lastname      String
  password      String
 


}
